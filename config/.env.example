# Environment Configuration
ENVIRONMENT=development
DEBUG=false

# SEC EDGAR Configuration
SEC_USER_AGENT=YourCompany YourEmail@example.com
SEC_RATE_LIMIT=10
SEC_RATE_PERIOD_SECONDS=1
SEC_TIMEOUT=30

# Database - Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database - DuckDB
DUCKDB_PATH=./data/sec_filings.duckdb
DUCKDB_MEMORY_LIMIT=4GB

# AI Models - Claude
SONNET_ENDPOINT=https://api.anthropic.com/v1
SONNET_API_KEY=your-sonnet-api-key

HAIKU_ENDPOINT=https://api.anthropic.com/v1
HAIKU_API_KEY=your-haiku-api-key

OPUS_ENDPOINT=https://api.anthropic.com/v1
OPUS_API_KEY=your-opus-api-key

# Model Configuration
COMPLEXITY_THRESHOLD_HIGH=0.7
COMPLEXITY_THRESHOLD_MEDIUM=0.4
MODEL_MAX_RETRIES=3
MODEL_RETRY_DELAY=2

# Celery Configuration
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0
CELERY_WORKER_CONCURRENCY=4
CELERY_PREFETCH_MULTIPLIER=4
CELERY_RATE_LIMIT=100/m

# Signal Extraction
ENABLE_FINANCIAL_SIGNALS=true
ENABLE_SENTIMENT_SIGNALS=true
ENABLE_RISK_SIGNALS=true
ENABLE_MANAGEMENT_SIGNALS=true

# Monitoring
LOG_LEVEL=INFO
ENABLE_METRICS=true
METRICS_PORT=8000
ENABLE_ALERTS=true
ALERT_EMAIL=alerts@example.com
